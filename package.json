{
  "name": "pipedrive-embeddable-ringcentral-phone-spa",
  "version": "4.0.0",
  "description": "Add RingCentral Embeddable Voice widgets to Pipedrive",
  "keywords": [
    "RingCentral",
    "embeddable",
    "PipeDrive",
    "phone"
  ],
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config build/webpack.config.js --progress -w",
    "clean": "rm -rf dist",
    "start": "npm run build",
    "postinstall": "node build/post-install",
    "lint": "./node_modules/.bin/standard",
    "fix": "./node_modules/.bin/standard --fix",
    "down": "node build/download-adapter.js",
    "pack": "rm -rf dist *.zip && NODE_ENV=production ./node_modules/.bin/webpack --config build/webpack.config.js && npm run down && node build/pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringcentral/pipedrive-embeddable-ringcentral-phone-spa.git"
  },
  "author": "Drake Zhao <drake.zhao@ringcentral.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ringcentral/pipedrive-embeddable-ringcentral-phone-spa/issues"
  },
  "homepage": "https://github.com/ringcentral/pipedrive-embeddable-ringcentral-phone-spa#readme",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@electerm/antd-dayjs-webpack-plugin": "^1.0.7",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "json-deep-copy": "^1.1.8",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lodash-webpack-plugin": "^0.11.6",
    "postcss-loader": "^7.0.0",
    "postcss-rename-selector": "^0.1.1",
    "ringcentral-embeddable-extension-common": "^1.0.0-beta.0",
    "shelljs": "^0.8.5",
    "standard": "^17.0.0",
    "style-loader": "^3.3.1",
    "stylus": "^0.58.1",
    "stylus-loader": "^7.0.0",
    "timeout-as-promise": "^1.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  }
}
